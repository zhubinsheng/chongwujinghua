<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <de.hdodenhof.circleimageview.CircleImageView
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/profile_image"
        android:layout_width="96dp"
        android:layout_height="96dp"
        android:layout_gravity="center"
        android:layout_marginTop="50dp"
        android:src="@drawable/machine"
        android:background="@color/qmui_config_color_blue"
        app:civ_border_width="2dp"
        app:civ_border_color="@color/qmui_config_color_blue"/>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="15dp"
        android:gravity="center"
        android:textSize="18dp"
        android:text="name" />

    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:layout_marginTop="10dp"
        android:background="@color/btn_filled_blue_bg_disabled"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="60dp"
        android:orientation="vertical">

    <com.qmuiteam.qmui.widget.grouplist.QMUICommonListItemView
        android:id="@+id/group_list_item_kefu"
        android:layout_width="match_parent"
        android:layout_height="@dimen/qmui_list_item_height"
        app:qmui_accessory_type="chevron"
        app:qmui_orientation="vertical"
        app:qmui_commonList_titleColor="@color/qmui_s_link_color"
        app:qmui_commonList_detailColor="@color/qmui_config_color_red"/>

    <com.qmuiteam.qmui.widget.grouplist.QMUICommonListItemView
        android:id="@+id/group_list_item_shouhoufuwu"
        android:layout_width="match_parent"
        android:layout_height="@dimen/qmui_list_item_height"
        app:qmui_accessory_type="chevron"
        app:qmui_orientation="vertical"
        app:qmui_commonList_titleColor="@color/qmui_s_link_color"
        app:qmui_commonList_detailColor="@color/qmui_config_color_red"/>

    <com.qmuiteam.qmui.widget.grouplist.QMUICommonListItemView
        android:id="@+id/group_list_item_wodewuliu"
        android:layout_width="match_parent"
        android:layout_height="@dimen/qmui_list_item_height"
        app:qmui_accessory_type="chevron"
        app:qmui_orientation="vertical"
        app:qmui_commonList_titleColor="@color/qmui_s_link_color"
        app:qmui_commonList_detailColor="@color/qmui_config_color_red"/>

    <com.qmuiteam.qmui.widget.grouplist.QMUICommonListItemView
        android:id="@+id/group_list_item_changjianwenti"
        android:layout_width="match_parent"
        android:layout_height="@dimen/qmui_list_item_height"
        app:qmui_accessory_type="chevron"
        app:qmui_orientation="vertical"
        app:qmui_commonList_titleColor="@color/qmui_s_link_color"
        app:qmui_commonList_detailColor="@color/qmui_config_color_red"/>

    <com.qmuiteam.qmui.widget.grouplist.QMUICommonListItemView
        android:id="@+id/group_list_item_geiwomenliuyan"
        android:layout_width="match_parent"
        android:layout_height="@dimen/qmui_list_item_height"
        app:qmui_accessory_type="chevron"
        app:qmui_orientation="vertical"
        app:qmui_commonList_titleColor="@color/qmui_s_link_color"
        app:qmui_commonList_detailColor="@color/qmui_config_color_red"/>

    <com.qmuiteam.qmui.widget.grouplist.QMUICommonListItemView
        android:id="@+id/group_list_item_richangweihushouce"
        android:layout_width="match_parent"
        android:layout_height="@dimen/qmui_list_item_height"
        app:qmui_accessory_type="chevron"
        app:qmui_orientation="vertical"
        app:qmui_commonList_titleColor="@color/qmui_s_link_color"
        app:qmui_commonList_detailColor="@color/qmui_config_color_red"/>

    </LinearLayout>
</LinearLayout>